{
  "name": "mz-studio/wallet_one",
  "description": "A PHP wrapper for using Wallet One API",
  "type": "yii2-extension",
  "keywords": [
    "Wallet One",
    "w1",
    "wrapper",
    "api"
  ],
  "homepage": "https://github.com/MZlabb/wallet_one",
  "license": "MIT",
  "version": "1.0.0",
  "authors": [
    {
      "name": "Zapeka Mykola",
      "email": "mzapeka@gmail.com",
      "role": "developer"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.0.0",
    "yiisoft/yii2": "2.0.15.1",
    "bower-asset/jquery": "3.2.*@stable",
    "guzzlehttp/guzzle": "~6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "autoload": {
    "psr-4": {
      "WalletOne\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WalletOne\\Test\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
